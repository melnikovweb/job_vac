{% if items %}
  <ul>
    {% for item in items %}
      <li class="{{ item.classes|join(' ') }}">
        <a target="{{ item.target }}" href="{{ item.link }}">{{ item.title}} </a>

        {% if (item.children|length) > 0 %}
          {% include 'parts/menu.twig' with { 'items': item.children } %}
        {% endif %}
      </li>
    {% endfor %}
  </ul>
{% endif %}
