{
  "name": "skeleton-theme",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "browser-sync": "^2.27.5",
    "browser-sync-webpack-plugin": "^2.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "del-cli": "^3.0.1",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-vue": "^7.14.0",
    "eslint-webpack-plugin": "^3.0.1",
    "laravel-mix": "^6.0.16",
    "laravel-mix-assets-splitter": "^1.0.9",
    "laravel-mix-eslint": "^0.2.0",
    "laravel-mix-php-manifest": "^1.0.1",
    "laravel-mix-serve": "^2.2.1",
    "laravel-mix-simple-output": "^1.0.1",
    "laravel-mix-stylelint": "^0.2.0",
    "laravel-mix-versionhash": "^2.0.1",
    "node-notifier": "^10.0.0",
    "node-sass-glob-importer": "^5.3.2",
    "postcss": "^8.3.6",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "webpack": "^5.46.0"
  },
  "scripts": {
    "development": "mix",
    "predevelopment": "del-cli public -f",
    "watch": "mix watch",
    "prewatch": "del-cli public -f",
    "hot": "mix watch --hot",
    "production": "mix --production",
    "preproduction": "del-cli public -f",
    "preinit-import-db": "wp db create",
    "init-import-db": "env-cmd -f ../../../.env cross-env-shell wp db import ../../../wp-database/$DB_NAME.sql",
    "export-db": "env-cmd -f ../../../.env cross-env-shell wp db export ../../../wp-database/$DB_NAME.sql",
    "preimport-db": "wp db clean --yes",
    "import-db": "env-cmd -f ../../../.env cross-env-shell wp db import ../../../wp-database/$DB_NAME.sql",
    "commit-db": "env-cmd -f ../../../.env cross-env-shell git add ../../../wp-database/$DB_NAME.sql && git commit -m update-db && git push",
    "open": "env-cmd -f ../../../.env cross-env-shell start $WP_SITEURL",
    "create-pages": "wp post create --post_type=page --post_status=publish --post_title=style-guide && wp post create --post_type=page --post_status=publish --post_title=gutenberg-blocks && wp post create --post_type=page --post_status=publish --post_title=dev-components",
    "rollout": "composer install && npm install && npm run init-import-db && npm create-pages && npm run development && npm run open"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fslightbox": "^3.4.1",
    "gsap": "^3.12.1",
    "jquery": "^3.6.0",
    "simplebar": "^6.2.5",
    "swiper": "^9.4.1"
  }
}
